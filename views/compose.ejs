<%- include('partials/header') -%>

 <h1>Compose</h1>
 <form action="/compose" method="post" class="row">
    <div class="form-group">
      <label for="postTitle">Title</label>  
      <input class="form-control" name="postTitle" id="postTitle" autocomplete="off" required></input>
      <label for="postBody">Post</label>
      <textarea class="form-control" name="postBody" id="postBody" cols="30" rows="5" required></textarea>  
    </div>
    <button type="submit" class="btn btn-primary">Publish</button>
 </form>

<%- include('partials/footer') -%>